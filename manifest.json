{
    "name": "nori",
    "description": "nori sample extension",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",  "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://172.16.10.114:6800/*",
                "https://127.0.0.1:8080/*"
            ],
            "js": [
                "extension.js"
            ]
        }
    ],
    "manifest_version": 2
}